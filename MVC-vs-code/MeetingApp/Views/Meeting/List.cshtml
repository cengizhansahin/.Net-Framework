@{
  ViewBag.Title="Katılımcı Listesi";
  
}
@model List<ApplyInfo>


<h1>Toplantıya katılım listesi</h1>
<table class="table table-bordered table-striped table-hover">
  <thead>
    <tr>
      <th>Ad</th>
      <th>Email</th>
      <th>Katılım</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    @foreach(var item in Model){
      <tr>
        <td>@item.Name</td>
        <td>@item.Phone</td>
        <td>@item.Email</td>
        <td>@(item.WillAttend?"Evet":"Hayır")</td>
      </tr>
    }
  </tbody>
</table>
